{
    "name":"Preprompt",
    "version":"0.0.1",
    "manifest_version":3,
    "action":{
        "default_popup":"popup.html",
        "default_icon":"logo.png"
    },
    "icons":{
        "128":"logo.png"
    },
    "permissions": ["tabs", "activeTab","storage", "scripting"],
    "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*" , "https://chat.openai.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}